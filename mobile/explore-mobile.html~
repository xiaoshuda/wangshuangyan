    <script type="text/javascript">
        $(function () {
            $(document).bind("wijappviewpageload", function (e, data) {
                var page = data.page,
                    title = page.jqmData("title"),
                    url = page.data("url");
                if (url.match(/index.html$/)) {
                    return;
                }
                // add a header if absent
                if (title && !page.find(":jqmData(role=header)").length) {
                    $('<div data-role="header"/>')
                        .append($('<a data-icon="back">Back</a>').attr({
                            // make it absolute for IE7
                            href: $.mobile.path.makeUrlAbsolute("index.html", url)
                        }))
                        .append($("<h2/>").text(title))
                        .prependTo(page);
                }
            });
        });
    </script>
    <style type="text/css">
        .v3-adaptive img
        {
            width: 100%;
        }
    </style>
